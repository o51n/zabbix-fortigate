<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-01-28T11:13:52Z</date>
    <groups>
        <group>
            <name>Networking Hardware SNMPv3</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>TemplateSNMPv3Fortigate</template>
            <name>Template SNMPv3 Fortigate 100</name>
            <groups>
                <group>
                    <name>Networking Hardware SNMPv3</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Availability</name>
                </application>
                <application>
                    <name>General</name>
                </application>
                <application>
                    <name>High Availability</name>
                </application>
                <application>
                    <name>Inventory</name>
                </application>
                <application>
                    <name>Network</name>
                </application>
                <application>
                    <name>VPN</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Configuration of an automatic configuration synchronization</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.13.1.5.0</snmp_oid>
                    <key>fgHaAutoSync</key>
                    <delay>5m</delay>
                    <applications>
                        <application>
                            <name>High Availability</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>HA cluster group name</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.13.1.7.0</snmp_oid>
                    <key>fgHaGroupName</key>
                    <delay>5m</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>High Availability</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>HA clustering priority</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.13.1.3.0</snmp_oid>
                    <key>fgHaPriority</key>
                    <delay>5m</delay>
                    <applications>
                        <application>
                            <name>High Availability</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Load-balancing schedule of cluster</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.13.1.6.0</snmp_oid>
                    <key>fgHaSchedule</key>
                    <delay>5m</delay>
                    <applications>
                        <application>
                            <name>High Availability</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>HA mode</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.13.1.1.0</snmp_oid>
                    <key>fgHaSystemMode</key>
                    <delay>2m</delay>
                    <applications>
                        <application>
                            <name>High Availability</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Location</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.6.0</snmp_oid>
                    <key>fortigateLocation</key>
                    <delay>5m</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Current connections</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.8.0</snmp_oid>
                    <key>fortinetCurrentConnections</key>
                    <delay>2m</delay>
                    <history>30d</history>
                    <units>C</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Current CPU Util</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.3.0</snmp_oid>
                    <key>fortinetCurrentCPUUtil</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
                            <params/>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{avg(#5)}&gt;95</expression>
                            <name>Fortinet {HOSTNAME} - Usage of CPU over 95%</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Current RAM Usage</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.4.0</snmp_oid>
                    <key>fortinetCurrentRAMUtil</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{avg(#5)}&gt;100</expression>
                            <name>Fortinet {HOSTNAME} - Memory Usage Over 100%</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Total storage space</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.7.0</snmp_oid>
                    <key>fortinetTotalStorage</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>1000000</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Fortinet Uptime</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.20.0</snmp_oid>
                    <key>fortinetUpTime</key>
                    <delay>2m</delay>
                    <history>30d</history>
                    <units>uptime</units>
                    <applications>
                        <application>
                            <name>Availability</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>1e-2</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last(#10)}&gt;{last(0)}</expression>
                            <name>Fortinet {HOST.NAME} Rebooted</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Used storage space</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.6.0</snmp_oid>
                    <key>fortinetUsedStorage</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>1000000</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Fortinet Used Storage %</name>
                    <type>CALCULATED</type>
                    <key>fortinetUsedStorage-percent</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <params>last(&quot;fortinetUsedStorage&quot;)/(last(&quot;fortinetTotalStorage&quot;)+0.0001)*100</params>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>ICMP Ping Secondary PE</name>
                    <type>SIMPLE</type>
                    <key>icmpping[{$PE_IP2}]</key>
                    <applications>
                        <application>
                            <name>Availability</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{max(#3)}=0</expression>
                            <name>Unavailable Secondary PE Ping</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>ICMP Ping PE</name>
                    <type>SIMPLE</type>
                    <key>icmpping[{$PE_IP}]</key>
                    <applications>
                        <application>
                            <name>Availability</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{max(#3)}=0</expression>
                            <name>Unavailable PE Ping</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Hostname</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.2.1.1.5.0</snmp_oid>
                    <key>SysHostname</key>
                    <delay>30m</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>NAME</inventory_link>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Firmware Version</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.1.0</snmp_oid>
                    <key>SysmFirmwareVersion</key>
                    <delay>3600</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>OS</inventory_link>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Firmware has been updated</name>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Hardware Model</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.2.1.47.1.1.1.1.7.1</snmp_oid>
                    <key>SysmHardwareModel</key>
                    <delay>30m</delay>
                    <history>2w</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>HARDWARE</inventory_link>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Serial Number</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.12356.100.1.1.1.0</snmp_oid>
                    <key>SysmSerialNumber</key>
                    <delay>3600</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>SERIALNO_A</inventory_link>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>High Availability</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.12356.101.13.2.1.1.11]</snmp_oid>
                    <key>fgHaStatsHostname</key>
                    <delay>300</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Number of anti-virus events triggered on cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsAvCount[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>CPU usage for cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsCpuUsage[{#SNMPVALUE}]</key>
                            <delay>2m</delay>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{avg(600)}&gt;80</expression>
                                    <name>CPU usage for unit {#SNMPVALUE} exceeded 80%</name>
                                    <priority>AVERAGE</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Number of IDS/IPS events triggered on cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.9.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsIdsCount[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Memory usage for cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsMemUsage[{#SNMPVALUE}]</key>
                            <delay>2m</delay>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{avg(600)}&gt;80</expression>
                                    <name>Memory usage for unit {#SNMPVALUE} exceeded 80%</name>
                                    <priority>AVERAGE</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Network bandwidth usage for unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsNetUsage[{#SNMPVALUE}]</key>
                            <delay>2m</delay>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Current session count for cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsSessCount[{#SNMPVALUE}]</key>
                            <delay>300</delay>
                            <units>sess</units>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Sync status on cluster unit $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.13.2.1.1.12.{#SNMPINDEX}</snmp_oid>
                            <key>fgHaStatsSyncStatus[{#SNMPVALUE}]</key>
                            <delay>2m</delay>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>High Availability</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>VPN Tunnel</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.12356.101.12.2.2.1.2]</snmp_oid>
                    <key>fgVpnTunEntPhase1Name</key>
                    <delay>300</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Number of bytes received on VPN $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.12.2.2.1.18.{#SNMPINDEX}</snmp_oid>
                            <key>fgVpnTunEntInOctets[{#SNMPVALUE}]</key>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>VPN</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Local IP address for VPN $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.12.2.2.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>fgVpnTunEntLocGwyIp[{#SNMPVALUE}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>VPN</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Number of bytes sent on VPN $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.12.2.2.1.19.{#SNMPINDEX}</snmp_oid>
                            <key>fgVpnTunEntOutOctets[{#SNMPVALUE}]</key>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>VPN</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Remote IP address for VPN $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.12356.101.12.2.2.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>fgVpnTunEntRemGwyIp[{#SNMPVALUE}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>VPN</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Current status for VPN $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.12356.101.12.2.2.1.20.{#SNMPINDEX}</snmp_oid>
                            <key>fgVpnTunEntStatus[{#SNMPVALUE}]</key>
                            <applications>
                                <application>
                                    <name>VPN</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Network Interfaces</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.31.1.1.1.1,{#IFALIAS},.1.3.6.1.2.1.31.1.1.1.18]</snmp_oid>
                    <key>ifname</key>
                    <delay>300</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#IFALIAS}</macro>
                                <value>.*</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Link Speed {#SNMPVALUE} {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>ifHighSpeed[{#SNMPVALUE}]</key>
                            <delay>300</delay>
                            <history>2w</history>
                            <applications>
                                <application>
                                    <name>Network</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Download {#SNMPVALUE} {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>ifInOctets[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <history>2w</history>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Network</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Operational status {#SNMPVALUE} {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.8.{#SNMPINDEX}</snmp_oid>
                            <key>ifInOperStatus[{#SNMPVALUE}]</key>
                            <applications>
                                <application>
                                    <name>Network</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Upload {#SNMPVALUE} {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.16.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutOctets[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <history>2w</history>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Network</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Network Traffic {#SNMPVALUE}</name>
                            <graph_items>
                                <graph_item>
                                    <color>199C0D</color>
                                    <item>
                                        <host>TemplateSNMPv3Fortigate</host>
                                        <key>ifInOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>F63100</color>
                                    <item>
                                        <host>TemplateSNMPv3Fortigate</host>
                                        <key>ifOutOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
